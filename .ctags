--langdef=smali
--langmap=smali:.smali
--regex-smali=/^\.field (public )?(private )?(protected )?(static )?(final )?(synthetic )?([^:]*):.*/\7/f,field/
--regex-smali=/^\.method (public )?(private )?(protected )?(static )?(final )?(varargs )?(bridge )?(synthetic )?(.*)\(.*/\9/m,method/

--langdef=markdown
--langmap=markdown:.md
--regex-markdown=/^#{1}(?!include)[ \t]*([^#]+.*)/\2/h,Heading_L1/
--regex-markdown=/^#{2}[ \t]*([^#]+.*)/\1/i,Heading_L2/
--regex-markdown=/^#{3}[ \t]*([^#]+.*)/\1/j,Heading_L3/
--regex-markdown=/^#{4}[ \t]*([^#]+.*)/\1/k,Heading_L4/
--regex-markdown=/^#{5}[ \t]*([^#]+.*)/\1/l,Heading_L5/
--regex-markdown=/^#{6}[ \t]*([^#]+.*)/\1/m,Heading_L6/
